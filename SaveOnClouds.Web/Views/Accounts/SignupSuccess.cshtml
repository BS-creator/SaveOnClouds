@model SaveOnClouds.Web.Models.Accounts.SignupSuccessViewModel

@{
    ViewData["Title"] = "SignupSuccess";
}

<h1>SignupSuccess</h1>

<div>
    <h4>SignupSuccessViewModel</h4>
    <hr />
    <dl class="row">
        <dd class="col-sm-10">
            A confirmation Email has been sent to: @Html.DisplayFor(model => model.Email), please check your inbox
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("SignIn", "SignIn")
</div>
